{% extends 'bo/base.html.twig' %}

{% block body %}

<h1>Générer une licence pour {{ societe.raisonSociale }}</h1>

<a class="d-block mb-4" href="{{ path('app_bo_societes') }}">
    <i class="fa fa-chevron-left" aria-hidden="true"></i>
    Retourner à la liste des sociétés
</a>

    <h3 class="text-center">{{ license.name }}</h3>

{{ form_start(form) }}
