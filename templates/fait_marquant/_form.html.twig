{{ form_start(form) }}

<div class="row">
    {{ form_errors(form) }}
    <div class="col-md-9 col-sm-12">
        {{ form_row(form.titre) }}
    </div>
    <div class="col-md-3 col-sm-12">
        {{ form_row(form.date) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.description) }}
        <span id="custom_ckeditor_max_char" class="d-none"
              data-limit="{{ userContext.societeUser.societe.faitMarquantMaxDesc }}"
              data-limitBlocking="{{ userContext.societeUser.societe.faitMarquantMaxDescIsblocking }}"></span>
    </div>
    <div class="col-md-12">
        {{ form_row(form.sendedToEmails) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.extraSendedToEmails) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.fichierProjets) }}
    </div>
    <div class="col-sm-12 text-center mt-2">
        <button class="btn btn-success">
            {{ 'publish' | trans }}
        </button>
    </div>
</div>

{{ form_end(form) }}